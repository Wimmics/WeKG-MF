@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix weo: <http://ns.inria.fr/meteo/ontology/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geosparql: <http://www.opengis.net/ont/geosparql#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix dcterms: <http://purl.org/dcterms/terms/> .
@prefix time: <http://www.w3.org/2006/time#>. 


<http://ns.inria.fr/meteo/ontology/WeatherStation>
  rdfs:subclassof sosa:Platform, <http://www.opengis.net/ont/geosparql#Feature> ;
  geosparql:hasGeometry [
    a geosparql:Geometry ;
    geosparql:asWKT geosparql:wktLiteral
  ] ;
  rdfs:label xsd:string ;
  geo:lat xsd:double ;
  geo:long xsd:double ;
  geo:alt xsd:int ;
  dcterms:spatial <http://www.wikidata.org/entity/Q2221906> .

  weo:Interval3h rdfs:subClassOf time:Interval; owl:equivalentClass [  
  a owl:Restriction; 
  owl:onProperty time:hasDuration ; 
  owl:hasValue [  
    a time:Duration ; 
    time:numericDuration  "3"^^xsd:decimal ; 
    time:unitType time:unitHour  
    ] 
].

weo:Interval24h rdfs:subClassOf time:Interval; owl:equivalentClass [  
  a owl:Restriction; 
  owl:onProperty time:hasDuration ; 
  owl:hasValue [  
    a time:Duration ; 
    time:numericDuration  "24"^^xsd:decimal ; 
    time:unitType time:unitHour  
    ] 
].

weo:Interval10min rdfs:subClassOf time:Interval; 
owl:equivalentClass [  
  a owl:Restriction; 
  owl:onProperty time:hasDuration ; 
  owl:hasValue [  
    a time:Duration ; 
    time:numericDuration "10"^^xsd:decimal ; 
    time:unitType time:unitMinute  
    ] 
].

<http://ns.inria.fr/meteo/ontology/unit/Okta>
  a <http://qudt.org/schema/qudt/Unit> ;
  <http://qudt.org/schema/qudt/plainTextDescription> "In meteorology, an okta is a unit of measurement used to describe the amount of cloud cover at any given location such as a weather station." ;
  <http://qudt.org/schema/qudt/ucumCode> "okta"^^<http://qudt.org/schema/qudt/UCUMcs> ;
  rdfs:label "Okta"@en ;
  rdfs:label "Octa"@fr .

weo:QualityValue a owl:Class; rdfs:subClassOf qudt:Concept .

<weo:property/stateGround> a <http://ns.inria.fr/meteo/ontology/WeatherProperty> ;
ssn:isPropertyOf <http://ns.inria.fr/meteo/ontology/featureOfInterest/surface> ;
rdfs:label "Etat du sol"@fr ;
weo:hasAbbreviation "etat_sol" .


<http://ns.inria.fr/meteo/ontology/MeteorologicalFeature> rdfs:subClassOf sosa:FeatureOfInterest.

<http://ns.inria.fr/meteo/ontology/WeatherProperty> rdfs:subClassOf sosa:ObservableProperty.

<http://ns.inria.fr/meteo/ontology/MeteorologicalObservation> rdfs:subClassOf sosa:Observation; 
rdfs:subClassOf [ a owl:Restriction ;
                        owl:onProperty sosa:observedProperty ;
                        owl:allValuesFrom weo:WeatherProperty
                    ] ;
rdfs:subClassOf [ a owl:Restriction ;
                        owl:onProperty sosa:hasFeatureOfInterest ;
                        owl:allValuesFrom weo:MeteorologicalFeature
                    ].

<http://ns.inria.fr/meteo/ontology/featureOfInterest/wind> a weo:MeteorologicalFeature;
skos:exactMatch <http://ontology.irstea.fr/weather/resource/featureOfInterest/wind>, <https://dbpedia.org/page/Category:Wind>;
ssn:hasProperty  <http://ns.inria.fr/meteo/ontology/property/windAverageSpeed>,
<http://ns.inria.fr/meteo/ontology/property/windAverageDirection> .


<http://ns.inria.fr/meteo/ontology/featureOfInterest/gust> a weo:MeteorologicalFeature, skos:Concept;
ssn:hasProperty  <http://ns.inria.fr/meteo/ontology/property/gustSpeed>, <http://ns.inria.fr/meteo/ontology/property/windSpeedOfGustInPeriod>  .

<http://ns.inria.fr/meteo/ontology/featureOfInterest/precipitation> a weo:MeteorologicalFeature;
skos:exactMatch <http://ontology.irstea.fr/weather/resource/featureOfInterest/precipitation> ;
ssn:hasProperty <weo:property/precipitationAmount> .

<http://ns.inria.fr/meteo/ontology/featureOfInterest/air> a weo:MeteorologicalFeature;
skos:exactMatch <http://ontology.irstea.fr/weather/resource/featureOfInterest/air>; 
ssn:hasProperty <weo:property/temperature>, <weo:property/dewPoint>, <weo:property/minTemperature>, <weo:property/maxTemperature>, <weo:property/wetBulbTemperature>, <weo:property/humidity>,
<weo:property/stationPressure>, <weo:property/differentialPressure>, <weo:property/seaLevelPressure>. 

<http://ns.inria.fr/meteo/ontology/featureOfInterest/cloud> a weo:MeteorologicalFeature;
ssn:hasProperty <weo:property/totalCloudCover>, <weo:property/cloudBaseAltitude>, <weo:property/lowCloudCover> . 

<http://ns.inria.fr/meteo/ontology/featureOfInterest/surface> a weo:MeteorologicalFeature; 
ssn:hasProperty <weo:property/soilMinTemperature>, <weo:property/freshSnowDepth>, <weo:property/totalCoverHeight>, <weo:property/stateGround> .


<http://ns.inria.fr/meteo/ontology/property/windAverageSpeed>
